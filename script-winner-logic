            return
        } else if (tile == bottomRight && turnCounter.position.bottomRight == false){
            player1.position.bottomRight = true;
            turnCounter.position.bottomRight = true;
            player1After();
            winnerTest(player1);
            player1.turn = false;
            player2.turn = true;
            return
        } else {
            return
        }
    } else {
        return
    }
    return
};
